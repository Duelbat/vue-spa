{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm run build-vendor && npm run build",
    "dev": "webpack-dev-server",
    "watch": "webpack --config webpack.config.js --watch",
    "build": "webpack --config webpack.config.js",
    "build-prod": "webpack --config webpack.config.js --env.prod",
    "build-vendor": "webpack --config webpack.config.vendor.js",
    "publish": "webpack --config webpack.config.vendor.js --env.prod && npm run build-prod && dotnet publish -c Release",
    "test": "karma start karma.conf.js",
    "test-watch": "karma start karma.conf.js --no-single-run --auto-watch",
    "typescript-ref": "cd src && typescript-ref"
  },
  "karma": {
    "globals": {
      "BaseUrl": "http://localhost:5000"
    }
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "es6-shim": "^0.35.3",
    "font-awesome": "^4.7.0",
    "servicestack-client": "0.0.40",
    "vue": "^2.5.2",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "@types/sinon": "^2.2.1",
    "@types/sinon-chai": "^2.7.27",
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.1.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gulp": "~3.9.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.7.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.5.0",
    "karma-sinon-chai": "^1.3.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.5",
    "lodash": "^4.17.4",
    "mocha": "^3.4.1",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^2.0.5",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.9.5",
    "run-sequence": "^1.2.2",
    "sass-loader": "^6.0.5",
    "servicestack-cli": "^0.0.5",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.17.0",
    "ts-loader": "^2.0.3",
    "typescript": "^2.5.3",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
